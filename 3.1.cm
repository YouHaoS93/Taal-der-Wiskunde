public class Mathpow 
{
  	public static void main(String[] args)
		{ 
	
		double[] a = {5, 3, .5, 4, 6}
		double[] b = {4, 2, 7, .5, 3}
		double[] c = {8, .5, 9, 6, 1}
	
		for (int i = 0; i<=4; i++)
		{
	
		double d = Math.sqrt( (Math.pow b, 2) - (4 * a[i] * c[i]) )
		double a2 = 2 * a[i]
	
		double x1 = ( (-b) + d ) / a2 	//positief
		double x2 = ( (-b) - d ) / a2	//negatief
	
		System.out.println("antwoord " +a+"\t"+b+"\t"+c+"\t"+x1 +"\t"+ x2 +"\t")
		}
	}
}
